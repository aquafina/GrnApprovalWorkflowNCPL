<process name="BPELProcessUpdFlag_CaseDisapproved"
               targetNamespace="http://xmlns.oracle.com/GrnApprovalWorkflow/GrnApproval/BPELProcessUpdFlag_CaseDisapproved"
               xmlns="http://schemas.xmlsoap.org/ws/2003/03/business-process/"
               xmlns:client="http://xmlns.oracle.com/GrnApprovalWorkflow/GrnApproval/BPELProcessUpdFlag_CaseDisapproved"
               xmlns:ora="http://schemas.oracle.com/xpath/extension"
               xmlns:bpelx="http://schemas.oracle.com/bpel/extension"
         xmlns:bpws="http://schemas.xmlsoap.org/ws/2003/03/business-process/"
         xmlns:ns1="http://xmlns.oracle.com/pcbpel/adapter/db/GrnApprovalWorkflow/GrnApproval/UpdFlagCaseDisapproved"
         xmlns:bpel2="http://docs.oasis-open.org/wsbpel/2.0/process/executable">

    <!-- 
      ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
        PARTNERLINKS                                                      
        List of services participating in this BPEL process               
      ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
    --> 
  <partnerLinks>
    <!-- 
      The 'client' role represents the requester of this service. It is 
      used for callback. The location and correlation information associated
      with the client role are automatically set using WS-Addressing.
    -->
    <partnerLink name="bpelprocessupdflag_casedisapproved_client" partnerLinkType="client:BPELProcessUpdFlag_CaseDisapproved" myRole="BPELProcessUpdFlag_CaseDisapprovedProvider"/>
    <partnerLink name="UpdFlagCaseDisapproved"
                 partnerLinkType="ns1:UpdFlagCaseDisapproved_plt"
                 partnerRole="UpdFlagCaseDisapproved_role"/>
  </partnerLinks>

  <!-- 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
      VARIABLES                                                        
      List of messages and XML documents used within this BPEL process 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  -->
  <variables>
    <!-- Reference to the message passed as input during initiation -->
    <variable name="inputVariable" messageType="client:BPELProcessUpdFlag_CaseDisapprovedRequestMessage"/>

    <!-- Reference to the message that will be returned to the requester-->
    <variable name="outputVariable" messageType="client:BPELProcessUpdFlag_CaseDisapprovedResponseMessage"/>
    <variable name="InvokeUpdFlagCaseDisapproved_input"
              messageType="ns1:UpdFlagCaseDisapprovedInput_msg"/>
  </variables>

  <!-- 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
     ORCHESTRATION LOGIC                                               
     Set of activities coordinating the flow of messages across the    
     services integrated within this business process                  
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  -->
  <sequence name="main">

    <!-- Receive input from requestor. (Note: This maps to operation defined in BPELProcessUpdFlag_CaseDisapproved.wsdl) -->
    <receive name="receiveInput" partnerLink="bpelprocessupdflag_casedisapproved_client" portType="client:BPELProcessUpdFlag_CaseDisapproved" operation="process" variable="inputVariable" createInstance="yes"/>
    <assign name="Transform1">
      <bpelx:annotation>
        <bpelx:pattern>transformation</bpelx:pattern>
      </bpelx:annotation>
      <copy>
        <from expression="ora:doXSLTransformForDoc('xsl/Transformation_InputFlagCaseDisapproved.xsl', $inputVariable.payload)"/>
        <to variable="InvokeUpdFlagCaseDisapproved_input"
            part="UpdFlagCaseDisapprovedInput_msg"/>
      </copy>
    </assign>
    <invoke name="InvokeUpdFlagCaseDisapproved"
            inputVariable="InvokeUpdFlagCaseDisapproved_input"
            partnerLink="UpdFlagCaseDisapproved"
            portType="ns1:UpdFlagCaseDisapproved_ptt"
            operation="UpdFlagCaseDisapproved" bpelx:invokeAsDetail="no"/>
    <!-- Generate reply to synchronous request -->
    <reply name="replyOutput" partnerLink="bpelprocessupdflag_casedisapproved_client" portType="client:BPELProcessUpdFlag_CaseDisapproved" operation="process" variable="outputVariable"/>
  </sequence>
</process>